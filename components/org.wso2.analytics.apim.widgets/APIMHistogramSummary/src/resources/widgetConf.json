          "tableName": "APIM_ReqCountAgg_MINUTES",
          "incrementalColumn": "AGG_TIMESTAMP",
          "publishingLimit": 2147483647,
          "purgingInterval": 6,
          "purgingLimit": 6,
          "isPurgingEnable": false
        }
      }
    },
    "options": [
      {
        "id" : "header",
        "title" : "Header",
        "type" : {
          "name" : "BOOLEAN",
          "possibleValues" : [
            true,
            false
          ]
        },
        "defaultValue": false
      }
    ]
  }
}
