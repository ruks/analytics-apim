{
  "name" : "APIM Histogram Summary",
  "id" : "APIMHistogramSummary",
  "thumbnailURL" : "",
  "configs" : {
    "pubsub" : {
      "types" : ["subscriber"]
    },
    "providerConfig" : {
      "configs" : {
